<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>

  <connectionStrings>
    <!---Cadena de Conexiones-->
    <add name="ConexionPrincipal" connectionString="Data Source=localhost, 1433;Initial Catalog=Test;User=sa;Password=*123Test*;" providerName="System.Data.SqlClient"/>
  </connectionStrings>

  <appSettings>
    <add key="FlagContraseniaEncriptada" value="0"/>
    <add key="Api" value="https://api.opendata.esett.com//EXP03/MBAOptions"/>
  </appSettings>

 
  
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p;%d{yyyy-MM-dd HH:mm:ss};%M;%m%n" />
      </layout>
    </appender>

    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="c:\Log\pruebaTest.log" />
      <appendToFile value="true" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p;%d{yyyy-MM-dd HH:mm:ss};%M;%m%n" />
      </layout>
    </appender>

    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="c:\Log\" />
      <datePattern value="'pruebaTest_'yyyyMMdd'.log'" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="20" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p;%d{yyyy-MM-dd HH:mm:ss};%M;%m%n" />
      </layout>
    </appender>

    <root>
      <level value="ALL" />
      <appender-ref ref="ConsoleAppender" />
      <appender-ref ref="FileAppender" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
</configuration>


 






